       <h1 class="heading">Member Registration</h1>
       <h3><p class="para">Fill ups your details!</p></h3>
       <div id="start-form">
            <form action="/crewform/create" id = "joinning" class="justify-content-center" method="POST">
                  <input name = "name" type="text" id="name" class="form"placeholder="Name*" required>
                   
                    <select name="whatiam" id="model" class="form"placeholder="What i am?*"  required>
                        <option value="What I am" selected >What i am?*</option>
                        <option value="Model" >Model</option>
                        <option value="Anchor">Anchor</option>
                        <option value="Promoter">Promoter</option>
                        <option value="Host/Hostesses">Host/Hostesses</option>
                        <option value="Volunteer">Volunteer</option>
                        <option value="Other">Other</option>
                    </select>
                    <select name="gender" id="model" class="form"placeholder="" required>
                        <option value="Gender" selected >Gender*</option>
                        <option value="Male" >Male</option>
                        <option value="Female">Female</option>
                        
                    </select>
                    <input name = "age" type="number" name="your age"  class="form"placeholder="Your Age*"  required>

                  <input name = "email" type="email" name="Myemail"  class="form"placeholder="Email*"  required>
              
              
               
                
         
              
            
                  <input name = "phone" type="tel" id="number" class="form"placeholder="Phone Number*"  required>
                  <input name = "whatsapp" type="tel" id="Whatsapp Number" class="form"placeholder="Whatsapp number*"  required>
                  <input name = "state" type="text" class="form"placeholder="State*"  required>
                  <input name = "city" type="text" class="form"placeholder="City*"  required>
                  <input name = "hearaboutus" type="text" id = "hear-about-us" class="form"placeholder="Where did you hear about us">
